<Window x:Class="View.WPFBrowserClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFBrowserClient.View.Pages"
        mc:Ignorable="d"
        Title="Scraper Browser" Height="820" Width="1024">

    <Grid>
        <Frame x:Name="mainFrame" Content="{Binding ActualPage}" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
